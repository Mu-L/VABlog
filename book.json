{
  "title": "秦城季&音视频",
  "author": "秦城季",
  "description": "未来那个项目都不应该缺少音视频！",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "styles": {
    "website": "./styles/website.css"
  },
  "structure": {
    "readme": "README.md"
  },
  "links": {
    "sidebar": {
      "秦城季&音视频": "https://github.com/xhunmon/VABlog"
    }
  },
  "plugins": [
    "-sharing",
    "splitter",
    "expandable-chapters-small",
    "anchors",
    "favicon-absolute",
    "github",
    "github-buttons",
    "donate",
    "sharing-plus",
    "anchor-navigation-ex",
    "highlight",
    "back-to-top-button",
    "code",
    "-lunr",
    "-search",
    "search-pro",
    "tbfed-pagefooter",
    "pageview-count",
    "auto-scroll-table",
    "lightbox",
    "hide-element",
    "valine",
    "katex",
    "embed-pdf",
    "mermaid-gb3",
    "image-wrapper"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/xhunmon"
    },
    "github-buttons": {
      "buttons": [
        {
          "user": "xhunmon",
          "repo": "VABlog",
          "type": "star",
          "count": true,
          "size": "small"
        },
        {
          "user": "xhunmon",
          "width": "160",
          "type": "follow",
          "count": true,
          "size": "small"
        }
      ]
    },
    "donate": {
      "alipay": "/source/images/alipayimg.jpeg",
      "wechat": "/source/images/wechatpayimg.jpeg",
      "wechatText": "微信",
      "title": "请作者喝杯coffee",
      "button": "捐赠",
      "alipayText": "支付宝"
    },
    "sharing": {
      "douban": false,
      "facebook": false,
      "google": false,
      "hatenaBookmark": false,
      "instapaper": false,
      "line": false,
      "linkedin": false,
      "messenger": false,
      "pocket": false,
      "qq": false,
      "qzone": false,
      "stumbleupon": false,
      "twitter": false,
      "viber": false,
      "vk": false,
      "weibo": false,
      "whatsapp": false,
      "all": [
        "google",
        "facebook",
        "weibo",
        "twitter",
        "qq",
        "qzone",
        "linkedin",
        "pocket"
      ]
    },
    "anchor-navigation-ex": {
      "showLevel": false
    },
    "favicon-absolute":{
      "favicon": "/source/images/favicon.png",
      "appleTouchIconPrecomposed152": "/apple-tou ch-icon-precomposed-152.png"
    },
    "fontSettings": {
      "size": 2
    },
    "hide-element": {
      "elements": [
        ".gitbook-link"
      ]
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy xhunmon 2022 ",
      "modify_label": "Update: ",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "valine": {
      "appId": "xMemPde6Lax4ffLfLBkuEi9t-gzGzoHsz",
      "appKey": "jIVEp2lShgotSTy0GalhgIzm"
    },
    "page-toc-button": {
      "maxTocDepth": 2,
      "minTocSize": 2
    }
  },
  "pdf": {
    "pageNumbers": true,
    "fontFamily": "Arial",
    "fontSize": 12,
    "paperSize": "a4",
    "margin": {
      "right": 62,
      "left": 62,
      "top": 56,
      "bottom": 56
    }
  }
}